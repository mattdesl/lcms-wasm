{
  "name": "lcms-wasm",
  "version": "1.0.3",
  "type": "module",
  "description": "LittleCMS in JS with WASM",
  "main": "./dist/lcms.js",
  "license": "MIT",
  "author": {
    "name": "Matt DesLauriers",
    "url": "https://github.com/mattdesl"
  },
  "devDependencies": {
    "esbuild": "^0.20.0"
  },
  "exports": {
    "import": "./dist/lcms.js",
    "require": "./dist/lcms.cjs"
  },
  "scripts": {
    "minify:js": "esbuild dist/lcms.js --format=esm --minify --sourcemap --outfile=dist/lcms.min.js",
    "build:cjs": "esbuild tmp/cjs-shim.js --bundle --platform=node --format=cjs --sourcemap --outfile=dist/lcms.cjs",
    "minify:cjs": "esbuild dist/lcms.cjs --format=cjs --minify --sourcemap --outfile=dist/lcms.min.cjs",
    "build": "./build.sh && npm run minify:js && npm run build:cjs && npm run minify:cjs"
  },
  "keywords": [
    "lcm",
    "lcms",
    "cmyk",
    "cmy",
    "xyz",
    "color",
    "cms",
    "colour",
    "conversion"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/mattdesl/lcms-wasm.git"
  },
  "homepage": "https://github.com/mattdesl/lcms-wasm",
  "bugs": {
    "url": "https://github.com/mattdesl/lcms-wasm/issues"
  },
  "dependencies": {
    "tape": "^5.9.0"
  }
}
